extends layout

block content
  header.w3-container(style='padding-top:22px')
    h5
      b
        i.fa.fa-dashboard
        |  My Dashboard
  .w3-row-padding.w3-margin-bottom
    div#dataIn(style="width: 800px; height:400px; overflow-y: scroll; background-color: white; border: 1px solid black;")

  script.
    document.addEventListener("DOMContentLoaded", function(event) {
      var socket = io.connect('//'+document.location.hostname+':'+document.location.port);
      socket.on('chartData', function (data) {
        console.log(data);

        $('#dataIn').append("<div>data: " + data.value + "</div>")
      });
      socket.on('ackConnect', function (data) {
        console.log(data);
      });
    });
